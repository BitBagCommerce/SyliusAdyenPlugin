{
    "name": "bitbag/sylius-adyen-plugin",
    "type": "sylius-plugin",
    "description": "BitBag Adyen plug-in for Sylius.",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "adyen"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "adyen/php-api-library": "^10.1",
        "nyholm/psr7": "^1.4",
        "sylius/sylius": "^1.9.0",
        "symfony/messenger": "~4.4",
        "sylius/refund-plugin": ">=v1.0.0-RC.11"
    },
    "require-dev": {
        "behat/behat": "^3.6.1",
        "behat/mink-selenium2-driver": "^1.4",
        "dmore/behat-chrome-extension": "^1.3",
        "dmore/chrome-mink-driver": "^2.7",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "^1.3",

        "lakion/mink-debug-extension": "^2.0.0",
        "phpspec/phpspec": "^7.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "0.12.74",
        "phpstan/phpstan-doctrine": "0.12.31",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "phpstan/phpstan-webmozart-assert": "0.12.12",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-symfony": "^2.3",
        "sensiolabs/security-checker": "^6.0",
        "sylius-labs/coding-standard": "^4.0",
        "symfony/browser-kit": "^4.4",
        "symfony/debug-bundle": "^4.4 || ^5.0",
        "symfony/dotenv": "^4.4 || ^5.0",
        "symfony/intl": "^4.4 || ^5.0",
        "symfony/web-profiler-bundle": "^4.4 || ^5.0",
        "symfony/web-server-bundle": "^4.4 || ^5.0",
        "vimeo/psalm": "^4.4.1",
        "symplify/easy-coding-standard": "^9.3, !=9.3.27"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "BitBag\\SyliusAdyenPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\BitBag\\SyliusAdyenPlugin\\": "tests/"
        },
        "classmap": [
            "tests/Application/Kernel.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "php bin/create_node_symlink.php"
        ]
    }
}
